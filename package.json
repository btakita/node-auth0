{
  "name": "auth0",
  "version": "3.2.0",
  "description": "SDK for Auth0 API v2",
  "main": "src/index.js",
  "module": "src/index.mjs",
  "sideEffects": false,
  "files": [
    "src"
  ],
  "scripts": {
    "test": "mocha --reporter spec './test/**/*.tests.js'",
    "test:ci": "nyc npm run test -- --forbid-only --reporter mocha-junit-reporter",
    "test:watch": "cross-env NODE_ENV=test mocha --timeout 5000 './test/**/*.tests.js' './test/*.tests.js' --watch",
    "jsdoc:generate": "jsdoc --configure .jsdoc.json --verbose",
    "precommit": "pretty-quick --staged",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auth0/node-auth0"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "auth0",
    "api"
  ],
  "author": "Auth0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/node-auth0/issues"
  },
  "homepage": "https://github.com/auth0/node-auth0",
  "dependencies": {
    "axios": "^1.3.4",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "lru-memoizer": "^2.2.0",
    "rest-facade": "^1.16.4",
    "retry": "^0.13.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^40.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jsdoc": "^4.0.2",
    "json-loader": "^0.5.7",
    "jws": "^4.0.0",
    "minami": "^1.2.3",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "nock": "^13.3.0",
    "nyc": "^15.1.0",
    "prettier": "2.8.7",
    "pretty-quick": "^3.1.3",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.2",
    "string-replace-webpack-plugin": "0.1.3",
    "superagent-proxy": "^3.0.0",
    "webpack": "^5.76.3"
  }
}